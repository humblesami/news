{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "auth": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/auth",
      "sourceRoot": "projects/auth/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "port":4201,
          "options": {
            "outputPath": "templates/auth",
            "index": "projects/auth/src/index.html",
            "main": "projects/auth/src/main.ts",
            "polyfills": "projects/auth/src/polyfills.ts",
            "tsConfig": "projects/auth/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/auth/src/favicon.ico",
              { "glob": "**/*", "input": "assets", "output": "static/assets" }
            ],
            "styles": [
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/auth/src/environments/environment.ts",
                  "with": "projects/auth/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "auth:build",
            "port": 4201
          },
          "configurations": {
            "production": {
              "browserTarget": "auth:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "auth:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/auth/src/test.ts",
            "polyfills": "projects/auth/src/polyfills.ts",
            "tsConfig": "projects/auth/tsconfig.spec.json",
            "karmaConfig": "projects/auth/karma.conf.js",
            "assets": [
              "projects/auth/src/favicon.ico",
              "projects/auth/src/assets"
            ],
            "styles": [
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/auth/tsconfig.app.json",
              "projects/auth/tsconfig.spec.json",
              "projects/auth/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/auth/e2e/protractor.conf.js",
            "devServerTarget": "auth:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "auth:serve:production"
            }
          }
        }
      }
    },
    "tools": {
      "projectType": "library",
      "root": "projects/tools",
      "sourceRoot": "projects/tools/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/tools/tsconfig.lib.json",
            "project": "projects/tools/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/tools/src/test.ts",
            "tsConfig": "projects/tools/tsconfig.spec.json",
            "karmaConfig": "projects/tools/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/tools/tsconfig.lib.json",
              "projects/tools/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "myapp": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/myapp",
      "sourceRoot": "projects/myapp/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/myapp",
            "index": "projects/myapp/src/index.html",
            "main": "projects/myapp/src/main.ts",
            "polyfills": "projects/myapp/src/polyfills.ts",
            "tsConfig": "projects/myapp/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/myapp/src/favicon.ico",
              "projects/myapp/src/assets"
            ],
            "styles": [
              "projects/myapp/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/myapp/src/environments/environment.ts",
                  "with": "projects/myapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "myapp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "myapp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "myapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/myapp/src/test.ts",
            "polyfills": "projects/myapp/src/polyfills.ts",
            "tsConfig": "projects/myapp/tsconfig.spec.json",
            "karmaConfig": "projects/myapp/karma.conf.js",
            "assets": [
              "projects/myapp/src/favicon.ico",
              "projects/myapp/src/assets"
            ],
            "styles": [
              "projects/myapp/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/myapp/tsconfig.app.json",
              "projects/myapp/tsconfig.spec.json",
              "projects/myapp/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/myapp/e2e/protractor.conf.js",
            "devServerTarget": "myapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "myapp:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "myapp"
}